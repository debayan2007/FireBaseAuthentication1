<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.debayanapps.firebaseauthentication1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FireBaseAuthentication1">
        <activity android:name="com.debayanapps.firebaseauthentication1.ChatActivity"></activity>
        <activity
            android:name="com.debayanapps.firebaseauthentication1.ForgotPassword"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name="com.debayanapps.firebaseauthentication1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.debayanapps.firebaseauthentication1.SignUp" />
        <activity android:name="com.debayanapps.firebaseauthentication1.SignIn"></activity>
    </application>

</manifest>